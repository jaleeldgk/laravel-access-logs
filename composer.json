{
    "name": "jaleeldgk/laravel-access-logs",
    "description": "A Laravel package for logging user requests, best way to log all requests for debugging purpose.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jaleel Ahmad",
            "email": "jaleel.ds20@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Jaleeldgk\\LaravelAccessLogs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaleeldgk\\LaravelAccessLogs\\LaravelAccessLogsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
